{
  "name": "pedroskakum/api-clientlayer",
  "description": "Laravel Api Implementation - Client layer",
  "keywords": [
    "laravel",
    "api",
    "client",
    "curl"
  ],
  "licence": "MIT",
  "authors": [
    {
      "name": "Pedro Paulo Skakum",
      "email": "pedroskakum@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "firebase/php-jwt": "^2.2.0",
    "anlutro/curl": "^1.3.1"
  },
  "autoload": {
    "psr-4": {
      "pedroskakum\\ApiClientLayer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pedroskakum\\Tests\\ApiClientLayer\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

